package vetor;
import java.util.Scanner;
public class Vetor {
	
	static Scanner entrada = new Scanner(System.in);
	public static void main(String[] args) {
		
		
		int[] num = new int[10];
		for (int i = 0; i < 10; i++) {
			
			System.out.println("Digite um numero: ");
			num[i] = entrada.nextInt();
		}
		for (int i = 0; i < 10; i++) {
			System.out.println(num[i]);
		}
		for (int i = 9; i >= 0; i--) {
			System.out.println(num[i]);
			
		}
	}
}