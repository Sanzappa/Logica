package model;
	

public class Notas {
	
	public String componente;
		public float[] notas = new float[3];
		
		public Notas(String componente, float notas[]) {
			
			this.componente = componente;
			this.notas = notas;
	}
		public float media (float notas[]) {
			
			float result = 0;
			
			for (int i = 0; i < notas.length; i++) {
				result += notas[i];
			}
			return result/3;
		}
}